name: Ships

entry_points:

  create_data:
    conda_env: processing_environment.yml
    parameters:
      time_gap: float
      dataset_name: str
    command: >
      python tests/create_data.py {time_gap} {dataset_name}

  test_time_gaps:
    conda_env: model_fitting_environment.yml
    parameters:
      time_gap: float
      dataset_name: str
    command: >
      python tests/fit_and_evaluate_model.py {time_gap} {dataset_name}
